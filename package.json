{
  "name": "food-ordering",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://hema-kg.github.io/food-ordering",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "multer": "^2.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "start": "concurrently \"npm run server:prod\" \"npm run client\"",
    "server:dev": "cd server && npm run dev",
    "server:prod": "cd server && npm run start",
    "client": "cd client && npm run dev",
    "build": "react-scripts build",
    "predeploy": "cd client && npm run build",
    "deploy": "npx gh-pages -d client/dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "gh-pages": "^6.3.0"
  }
}
